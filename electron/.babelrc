        "targets": {
          "electron": "27.0"
        },
        "useBuiltIns": "entry",
        "corejs": 3
    }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ]
}